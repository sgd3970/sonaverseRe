{
  "name": "temp_app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=3072 next dev --webpack",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:a11y": "playwright test accessibility.spec.ts",
    "lighthouse": "lhci autorun",
    "lighthouse:dev": "cross-env LHCI_BUILD_CONTEXT__CURRENT_HASH=local lhci autorun --collect.url=http://localhost:3000",
    "lighthouse:prod": "cross-env LHCI_BUILD_CONTEXT__CURRENT_HASH=local lhci autorun --collect.startServerCommand='npm run start'"
  },
