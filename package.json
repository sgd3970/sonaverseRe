    "next": "16.0.10",
    "next-i18next": "^15.4.3",
    "next-seo": "^7.0.1",
    "nodemailer": "^7.0.11",
    "plaiceholder": "^3.0.0",
    "rate-limiter-flexible": "^9.0.1",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
